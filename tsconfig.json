{
  "compilerOptions": {
    "paths": {
      "ai-cli": ["./src/index.js"]
    },
    "target": "esnext",
    "module": "node16",
    "moduleResolution": "node16",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noStrictGenericChecks": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react"
  },
  "outDir": "build",
  "include": ["src"]
}
